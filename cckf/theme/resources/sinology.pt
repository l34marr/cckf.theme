<script>
  function callback() {
    $('#one').find('.ap-caption').each(function() {
      if (!$(this).parents('.slide').hasClass('selected')) {
        $(this).fadeOut();
      } else {
        if (!$(this).is(':visible')) $(this).fadeIn();
      }
    });
  }
  $('#one').accordionPro({
    autoPlay : true,
    onSlideOpen : callback,
    onSlideClose : callback,
    theme : 'stitch',
    rounded : true
  }).find('.ap-caption:first').show();
</script>

